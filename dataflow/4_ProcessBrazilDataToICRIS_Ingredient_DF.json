				"          {originated(UING)},",
				"          {product(UING)},",
				"          {amount(UING)},",
				"          {formula(UING)},",
				"          {ordination(UING)},",
				"          {id(UFR)},",
				"          {originated(UFR)},",
				"          {color(UFR)},",
				"          {line(UFR)},",
				"          {subline(UFR)},",
				"          {undercoat(UFR)},",
